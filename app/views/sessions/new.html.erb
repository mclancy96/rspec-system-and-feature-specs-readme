<h1>Log In</h1>
<%= form_with url: login_path, method: :post, local: true do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email, autofocus: true %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Log In" %>
  </div>
<% end %>
<%= link_to "Sign up", new_user_path %>
